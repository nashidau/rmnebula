} def
/beginfrontfirst {
        leftsection minx sub  maxy topsection sub translate
        0 0 moveto
        downline
} def
/beginfrontsecond {
        rightwidth 2 div 0 translate
        0 0 moveto
        downline
        microtitle
        norm
} def

/topleft {
    minx maxy moveto
} def

/col2 {
    midx minx sub 0 translate
    topleft
} def

